{
  "expo": {
    "name": "sumi-rebuild",
    "slug": "sumi-rebuild",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sumirebuild",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sumi-android-icon.png",
        "backgroundColor": "#F6F7EB"
      },
      "package": "com.anonymous.sumisupabase",
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
            [
        "expo-splash-screen",
        {
          "image": "./assets/images/sumi-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#393E41"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5850018728161057~8035866098",
          "iosAppId": "ca-app-pub-5850018728161057~1130953490"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1b98754-df28-4059-9f13-d17aa786933d"
      }
    }
  }
}
